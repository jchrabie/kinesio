{
  "name": "diane-kinesio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm run env -s && ng serve --port=4201 --proxy-config proxy.conf.js",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate",
    "build": "ng build --prod --base-href https://jchrabie.github.io/kinesio/",
    "deploy": "npx angular-cli-ghpages --dir=dist",
    "dev:ssr": "ng run diane-kinesio:serve-ssr",
    "serve:ssr": "node dist/diane-kinesio/server/main.js",
    "build:ssr": "ng build --prod && ng run diane-kinesio:server:production",
    "prerender": "ng run diane-kinesio:prerender"
  },
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/localize": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/platform-server": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@fortawesome/fontawesome-free": "^5.1.0",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@ng-toolkit/universal": "^1.1.21",
    "@nguniversal/express-engine": "^11.1.2",
    "@ngx-translate/core": "^12.0.0",
    "angular-ismobile": "^1.0.0",
    "angulartics2": "^8.3.0",
    "bootstrap": "^4.1.1",
    "express": "^4.15.2",
    "intersection-observer": "^0.12.0",
    "ng-in-viewport": "^6.1.5",
    "ngx-device-detector": "^2.0.5",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.0",
    "@angular-devkit/schematics": "^11.2.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@nguniversal/builders": "^11.1.2",
    "@ngx-rocket/scripts": "^4.0.0",
    "@schematics/angular": "^11.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.1",
    "hads": "^2.0.1",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "stylelint": "~13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.16.0",
    "ts-node": "^8.8.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~4.0.7"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
